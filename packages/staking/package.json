{
  "name": "@unifiprotocol/staking",
  "version": "1.0.0",
  "description": "Unifi Protocol JS Staking SDK",
  "author": "Unifi Protocol",
  "repository": "git@github.com:unifiprotocol/unifi-sdk.git",
  "homepage": "https://github.com/unifiprotocol/unifi-sdk/tree/master/packages/staking-sdk",
  "license": "BSD-3-Clause",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepublish": "yarn build",
    "build": "yarn run clean && yarn run compile",
    "build:watch": "tsc --watch",
    "clean": "rm -rf ./dist && rm -rf tsconfig.tsbuildinfo",
    "compile": "tsc -b tsconfig.json",
    "lint": "eslint lib/**.ts",
    "prepublishOnly": "npm run lint"
  },
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "dependencies": {
    "@harmony-js/core": "^0.1.57",
    "@harmony-js/staking": "^0.1.56",
    "@harmony-js/utils": "^0.1.56",
    "@unifiprotocol/core-sdk": "^3.2.1",
    "ethers": "^5.2.0",
    "tronweb": "^3.2.7",
    "tslib": "^2.1.0"
  },
  "devDependencies": {
    "@types/bip39": "^3.0.0",
    "@types/hdkey": "^2.0.0",
    "@types/node": "^15.6.1",
    "@types/websocket": "^1.0.3",
    "eslint": "^7.27.0",
    "npm-run-all": "^4.1.5",
    "tronweb-typings": "^1.0.1",
    "tsconfig-replace-paths": "^0.0.5",
    "tslib": "^2.1.0",
    "typescript": "^4.2.4"
