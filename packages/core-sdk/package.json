{
  "name": "@unifiprotocol/core-sdk",
  "version": "2.2.4",
  "description": "Unifi Protocol JS Core SDK",
  "author": "Unifi Protocol",
  "repository": "git@github.com:unifiprotocol/unifi-sdk.git",
  "homepage": "https://github.com/unifiprotocol/unifi-sdk/tree/master/packages/core-sdk",
  "license": "BSD-3-Clause",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepublish": "yarn build",
    "build": "yarn run clean && yarn run compile",
    "build:watch": "tsc --watch",
    "tsc-replace-paths": "tsconfig-replace-paths --project tsconfig.json",
    "clean": "rm -rf ./dist && rm -rf tsconfig.tsbuildinfo",
    "compile": "tsc -b tsconfig.json",
    "lint": "eslint lib/**.ts",
    "prepublishOnly": "npm run lint"
  },
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "dependencies": {
    "bignumber.js": "^9.0.1",
    "ethereum-multicall": "^2.4.0",
    "ethers": "^5.2.0",
    "eventemitter3": "^4.0.7",
    "js-sha3": "^0.8.0",
    "promise-parallel-throttle": "^3.3.0",
    "tronweb": "^3.2.7",
    "tslib": "^2.1.0"
  },
  "devDependencies": {
    "@types/node": "^15.6.1",
    "eslint": "^7.27.0",
    "npm-run-all": "^4.1.5",
    "tsconfig-replace-paths": "^0.0.5",
    "tslib": "^2.1.0",
    "typescript": "^4.2.4"
  }
}
